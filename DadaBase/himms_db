LOCK TABLES `wp_wpfb_files` WRITE;
/*!40000 ALTER TABLE `wp_wpfb_files` DISABLE KEYS */;
/*!40000 ALTER TABLE `wp_wpfb_files` ENABLE KEYS */;
UNLOCK TABLES;

